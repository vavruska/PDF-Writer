unset exitExists oif {status} == 0  mkdir oendNewer o/init.a init.cif {status} != 0  set init initendNewer o/dispatch.a dispatch.c export.hif {status} != 0  set dispatch dispatchendNewer o/opts.a opts.c opts.hif {status} != 0  set opts optsendNewer o/export.a export.c export.h opts.h pdfgen.hif {status} != 0  set export exportendNewer o/pdfgen.a pdfgen.c pdfgen.hif {status} != 0  set pdfgen pdfgenendNewer o/strcasecmp.a strcasecmp.c strcasecmp.hif {status} != 0  set strcasecmp strcasecmpendset list ""set list {list} {dispatch} {export} {opts} {pdfgen} {strcasecmp}if "{list}" != ""    echo Compiling Babelfish translator    for i in {list}        compile +O -P {i}.c keep=o/{i}    end    echo Linking Babelfish translator    link -P o/init o/dispatch o/opts o/export o/pdfgen o/strcasecmp keep=o/babelendNewer o/exportOpt.a exportOpt.c opts.hif {status} != 0    echo "Compiling Export Option"    compile +O -P exportOpt.c keep=o/exportOpt    link -P o/exportOpt o/opts keep=o/exportOptendecho "Putting it all together"compile pdfrez.rez keep=PDF.Writerfiletype PDF.Writer $BE $4003